{
  "name": "ngoply-hashtag-monitor",
  "version": "1.0.0",
  "description": "Monitor hashtag #ngoply on social media and send notifications to Discord",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:test": "tsx src/test-mode.ts --test",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:discord": "tsx src/test-mode.ts --test",
    "check:rate-limit": "tsx src/rate-limit-check.ts --status",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "hashtag",
    "monitoring",
    "discord",
    "twitter",
    "social-media",
    "ngoply",
    "typescript",
    "vite"
  ],
  "author": "Adiit",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "cheerio": "^1.0.0-rc.12",
    "cron": "^3.1.6",
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/cron": "^2.0.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "tsx": "^4.6.0",
    "vite-node": "^1.0.0"
  }
}